namespace chatty;

enum DType: byte {
  DT_NONE = 0,
  DT_INT8 = 1,
  DT_UINT8 = 2,
  DT_INT16 = 3,
  DT_UINT16 = 4,
  DT_INT32 = 5,
  DT_UINT32 = 6,
  DT_INT64 = 7,
  DT_UINT64 = 8,
  DT_FLOAT32 = 9,
  DT_FLOAT16 = 10,
  DT_INT4 = 11,
}

table Tensor {
    name: string(required);
    shape: [int] (required);
    dtype: DType(required);
    offset: int64(required);
}

table Model {

}

root_type Model;