namespace chatty;

enum DType:byte {
    NONE = 0,
    INT8 = 1,
    UINT8 = 2,
    INT16 = 3,
    UINT16 = 4,
    INT32 = 5,
    UINT32 = 6,
    INT64 = 7,
    UINT64 = 8,
    FLOAT32 = 9,
    FLOAT16 = 10,
    FLOAT8 = 11,
    BFLOAT16 = 12,
    INT4 = 13,
}

enum ActivationBits:byte {
    INT4=0, INT8=1, INT16=2, FP16=3,
}

table Tensor {
    name: string(required);
    shape: [int] (required);
    dtype: DType(required);
    offset: int64(required);
}

table Model {

}

root_type Model;